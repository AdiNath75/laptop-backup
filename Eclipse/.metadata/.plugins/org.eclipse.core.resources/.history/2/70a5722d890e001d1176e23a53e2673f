package leetcodes;

public class ValidateIP {
	public static void main(String[] args) {
		String st = ".172.16.254.1.";
		int count=0;
		for(int i=0;i<st.length();i++) {
			if(st.charAt(i) == '.') {
				count++;
			}
		}
		String str = st.replace('.', '@');
		String[] arr = str.split("@", count+1);
		
		for (String a : arr)
            int num = Integer.parseInt(a);	
		
	}


}
