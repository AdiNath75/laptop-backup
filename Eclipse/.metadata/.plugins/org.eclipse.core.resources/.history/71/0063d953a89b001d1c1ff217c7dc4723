package patterns;

public class Pattern4 {
	public static void main(String[] args) {
		int num = 5;
		for(int i=1;i<=5;i++) {
			for(int j=1;j<=5;j++) {
				num--;
				if(i<=j) {
					System.out.print(num+" ");
					num--;
				}
				else {
					System.out.print("* ");
				}
				
				
			}
			System.out.println();
		}
	}
	

}
